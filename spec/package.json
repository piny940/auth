{
  "name": "spec",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@10.18.1",
  "scripts": {
    "compile": "tsp compile .",
    "dev": "tsp compile . --watch",
    "doc": "npx @redocly/cli preview -d schema/@typespec/openapi3",
    "export": "npx @redocly/cli build-docs schema/@typespec/openapi3/openapi.yaml -o out/index.html",
    "lint": "tsp format '**/*.tsp'",
    "lint-check": "tsp format --check '**/*.tsp'"
  },
  "peerDependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@redocly/cli": "2.3.1",
    "@typespec/compiler": "1.4.0",
    "@typespec/http": "1.4.0",
    "@typespec/openapi3": "1.4.0",
    "@typespec/prettier-plugin-typespec": "1.4.0",
    "@typespec/rest": "0.74.0"
  },
  "private": true
}
